<%- include('../partials/header') %>
    <div class="container">
        <% ideas.forEach(function(idea) { %>
        <div class="idea">
            <div class="image"><img src="<%= idea.image %>" alt="picture"></div>
            <div class="title"><%= idea.title %></div>
            <a href="/ideas/<%= idea._id %>" class="view-button">View Idea</a>
        </div>
        <% }); %>
    </div>
<%- include('../partials/footer') %>